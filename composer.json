{
    "name": "nicksagona/resistor-color-codes",
    "description": "Resistor Color Code Label Make",
    "keywords": [
        "resistor",
        "color codes",
        "label maker"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@nolainteractive.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "popphp/popphp": "^3.2.3",
        "popphp/pop-http": "^3.1.2",
        "popphp/pop-view": "^3.0.2",
        "popphp/pop-image": "^3.2.2",
        "popphp/pop-pdf": "^3.1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0.0"
    },
    "autoload": {
        "psr-4": {
            "Resistor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Resistor\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.x-dev"
        }
    }
